{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "dom",
      "esnext"
    ],
    "strict": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "jsx": "preserve",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "types": [
      "node"
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
